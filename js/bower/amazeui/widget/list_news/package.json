            "type": "text",
            "placeholder": "css类名，多个用空格分隔",
            "default": "",
            "required": false
        },

        "theme": {
            "name": "主题",
            "desc": "模块主题",
            "type": "select",
            "default": "default",
            "required": true,
            "dataList": "<%= pkg.themes %>"
        },

        "options": {
            "type": {
                "name": "类型",
                "desc": "列表类型：normal - 普通, thumb - 缩略图",
                "type": "select",
                "required": false,
                "default": "normal",
                "dataList": [
                    {
                        "value": "normal",
                        "title": "文字列表"
                    },
                    {
                        "value": "thumb",
                        "title": "带缩略图列表"
                    }
                ]
            },

            "thumbPosition": {
                "name": "缩略图位置",
                "desc": "缩略图相对于标题的位置",
                "type": "select",
                "required": "!!(options.type === 'thumb')",
                "default": "bottom-left",
                "dataList": [
                    {
                        "value": "top",
                        "title": "Top"
                    },
                    {
                        "value": "bottom-left",
                        "title": "Bottom-left"
                    },
                    {
                        "value": "bottom-right",
                        "title": "bottom-right"
                    },
                    {
                        "value": "left",
                        "title": "Left"
                    },
                    {
                        "value": "right",
                        "title": "Right"
                    }
                ]
            },
            "morePosition": {
                "name": "更多链接位置",
                "desc": "更多相对于列表的位置",
                "type": "select",
                "required": false,
                "default": "bottom",
                "dataList": [
                    {
                        "value": "top",
                        "title": "上方"
                    },
                    {
                        "value": "bottom",
                        "title": "下方"
                    }
                ]
            }
        },

        "content": {
            "type": "Object",
            "item": {
                "header": {
                    "type": "Object",
                    "item": {
                        "title": {
                            "type": "text",
